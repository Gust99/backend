{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": 0,
        "indent": 0,
        "space-before-function-paren": 0,
        "eol-last": 0,
        "no-useless-constructor": 0,
        "@typescript-eslint/no-var-requires": 0
    }
}
